    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "all": "0.0.0",
    "browser": "^0.2.6",
    "browserlist": "^1.0.1",
    "browserslist": "^4.5.5",
    "normalize-css": "^2.3.1",
    "stylelint": "^10.0.1"
  },

{
  "name": "switchClock",
  "version": "1.0.0",
  "description": "\"# reactFirstview\"",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/index.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "start": "babel js/index.js --watch --out-file js/index-compiled.js",
    "browsersync": "browser-sync start --server  --files css/*.css *.html",
    "scss": "node-sass  --output-style compact  -watch  -o css sass"
  },
